    <Entry>
      <ID>1</ID>
      <DefaultText>"Als Mitglied des Rates versichere ich euch, die Kreatur wird gefunden!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Wie bald?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Unsere Häuser stehen auf dem Spiel!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"Unsere *Leben* stehen auf dem Spiel!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
